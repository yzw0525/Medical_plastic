/* pages/personal/mymsg/mymsg.wxss */
.mymsg_li{width: 100%; padding: 10rpx 3% 20rpx;border-bottom:solid #eeeeee 1px;border-radius: 10rpx;margin:10rpx auto;}
.mymsg_li .mymsg-image{width: 150rpx;height: 150rpx;border:solid #eeeeee 1px;border-radius: 10rpx;overflow: hidden;float: left;text-align: center;display:flex;align-items: center;justify-content: center;}
.mymsg_li .mymsg-image image{width: 80%;margin: auto;}
.mymsg_li .mymsg-content{padding-left: 165rpx;height: 150rpx; position:relative;}
.mymsg_li .mymsg-title{font-size: 30rpx;color: #333;line-height: 40rpx;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;overflow: hidden;}
.mymsg_li .mymsg-time{font-size: 28rpx;color: #999;position: absolute;bottom: 0;}