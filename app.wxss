view,scroll-view,swiper,navigator,button{box-sizing: border-box;}
page{background-color: #f5f5f5;font-size: 26rpx;}
image{width: 100%;display:block;}
.clear::after{content: ""; clear:both;display: block; width: 0;height: 0;}
/* @media screen and ( resolution : 2dppx ) {
    .banner{height: 180px;border:2px solid teal;}
} */
 @media(max-width: 320px){
  .banner{height: 153.6px;}
}
@media (min-width: 321px) and (max-width: 375px)  {
  .banner{height: 180px;}
}
@media (min-width: 376px) and (max-width: 414px)  {
  .banner{height: 198.72px;}
}
@media (min-width: 750px){
  page{font-size: 24rpx;max-width: 750px;margin: auto;}
  .banner{width:750px; height: 360px;margin:auto;}
}
/***  ***/
.main{width: 100%;background-color: #fff;margin:10rpx auto 0;overflow: hidden;}
.main .title{height: 80rpx;line-height: 80rpx;font-size: 30rpx; padding: 0 3%;padding-left:70rpx;color:#333;position: relative;}
.main .title::before{content: "";display: block;width: 50rpx;height: 50rpx;position: absolute;left: 3%;top: 50%;margin-top: -25rpx;background-position: center;background-repeat: no-repeat;background-size: 80%;}
.main .title .more{color: #937343;position: absolute;top:0;right: 3%;}
/*** more ***/
.load_more{display: block;background-color: #fff; line-height: 50rpx;font-size: 26rpx;text-align: center;color: #bfa78b;position: relative;margin:auto;}
.load_more::after{display: block;content: ""; width: 50rpx;height: 50rpx;position: absolute;top: 0;right: 50%;background-image:url(https://xcx.yzw0525.com/images/index_16.svg);background-size: 60%;background-position: center; background-repeat: no-repeat;margin-right:-95rpx;}
.loadnull{width: 100%;margin:40rpx auto;height: 1px;font-size: 26rpx;text-align: center;background: -webkit-linear-gradient(left, #fff, #999 50%, #fff);}
.loadnull text{position: relative;line-height: 40rpx;top: -20rpx;padding:0 15rpx;background-color: #fff;color: #999999;display: inline-block;}
.loadnull text.f5{background-color: #f5f5f5;}
/*** more end ***/
/*** projectType ***/
.btn-list{padding: 10rpx 3%;display: flex;justify-content: space-between;flex-wrap: wrap;}
.btn-list > view{width: 25%;height: 140rpx;position: relative;margin: 10rpx auto;}
.btn-list > view::before{content:"";width: 100rpx;height: 100rpx;position: absolute;top: 0;left: 50%;margin-left: -50rpx;background-size: 80%;background-repeat: no-repeat;background-position: center;border-radius: 50rpx;}
.btn-list > view text{display: block;width: 100%;line-height: 40rpx;text-align: center;position: absolute;left: 0;bottom: 0;color: #333;font-size: 28rpx;}
.btn-list > view.active text{color: #937343;}
.btn-list > view.all::before{background-color: #fdedda;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_all.png);}
.btn-list > view.eye::before{background-color: #fdf0ef;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_eye.png);}
.btn-list > view.nose::before{background-color: #e8f0fa;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_nose.png);}
.btn-list > view.chest::before{background-color: #fdedda;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_chest.png);}
.btn-list > view.face::before{background-color: #fdf0ef;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_face.png);}
.btn-list > view.thin::before{background-color: #e8f0fa;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_thin.png);}
.btn-list > view.rhytidectomy::before{background-color: #fdedda;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_rhytidectomy.png);}
.btn-list > view.shaping::before{background-color: #dff0eb;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_shaping.png);}
.btn-list > view.beauty-freckle::before{background-color: #dff0eb;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_beauty-freckle.png);}
.btn-list > view.cosmetology::before{background-color: #fdf0ef;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_cosmetology.png);}
.btn-list > view.tattoo::before{background-color: #e8f0fa;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_tattoo.png);}
.btn-list > view.oral-cavity::before{background-color: #fdedda;background-image: url(http://hfyx.yzw0525.com/images/icon/icon_project_oral-cavity.png);}
/**** projectType end ***/
/**** icon ***/
icon{display: inline-block;width: 30rpx;height: 30rpx;background-position: center;background-size: 100%;background-repeat: no-repeat;}
icon.down{ background-image: url(http://hfyx.yzw0525.com/images/icon/icon_down.svg);}
icon.up{background-image: url(http://hfyx.yzw0525.com/images/icon/icon_up.svg);}
/**** icon end ***/
.wxParse-p{overflow: hidden;}
/**** BottomNavigation ***/
.BottomNavigation{width: 100%;position: fixed;bottom: 0;display:flex;justify-content:space-between;flex-wrap:wrap;}
.BottomNavigation .nav{width: 50%;line-height: 100rpx;text-align: center;color: #fff;font-size: 32rpx;letter-spacing: 4rpx;}
.BottomNavigation .nav text{position: relative;display: block;width: max-content;height: max-content; margin: auto;padding-left: 65rpx;}
.BottomNavigation .nav text::before{content: "";display: block; width: 60rpx;height: 60rpx;background-position: center;background-size: 90%;background-repeat: no-repeat;position: absolute;left: 0;top: 50%;margin-top: -30rpx;}
.BottomNavigation .nav.consultation{background-color: #000;border-radius: unset;}
.BottomNavigation .nav.consultation::after{display:none;}
.BottomNavigation .nav.consultation text::before{background-image: url(http://hfyx.yzw0525.com/images/icon/icon_consultation.svg);}
.BottomNavigation .nav.online{background-color: #937343;}
.BottomNavigation .nav.online text::before{background-image: url(http://hfyx.yzw0525.com/images/icon/icon_online.svg);}
/**** BottomNavigation end ***/

